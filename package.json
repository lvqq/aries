{
  "name": "@tooltik/aries",
  "type": "module",
  "version": "0.11.2",
  "description": "A swagger converter cli, support TypeScript declaration, request function, markdown docs, mock js and so on",
  "packageManager": "pnpm@7.9.3",
  "main": "./dist/src/index.cjs",
  "module": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "bin": {
    "aries": "./dist/bin/aries.js"
  },
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js",
      "require": "./dist/src/index.cjs"
    }
  },
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "dev": "rimraf dist && tsup --sourcemap --watch",
    "build": "rimraf dist && tsup --sourcemap",
    "build:production": "rimraf dist && tsup --treeshake",
    "prepare": "husky install",
    "prepublish": "pnpm build:production",
    "typecheck": "tsc",
    "lint": "eslint --fix --quiet --ext .ts src bin",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --silent",
    "test:ci": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lvqq/aries.git"
  },
  "author": "lvqq",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lvqq/aries/issues"
  },
  "homepage": "https://github.com/lvqq/aries#readme",
  "files": [
    "dist"
  ],
  "dependencies": {
    "axios": "^0.24.0",
    "chalk": "4.1.2",
    "chance": "^1.1.8",
    "commander": "^8.3.0",
    "cors": "^2.8.5",
    "esbuild": "^0.15.6",
    "express": "^4.17.2",
    "js-yaml": "^4.1.0",
    "lodash.frompairs": "^4.0.1",
    "lodash.groupby": "^4.6.0",
    "lodash.mapvalues": "^4.6.0",
    "ora": "5.4.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.0.2",
    "@types/chance": "^1.1.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.184",
    "@types/lodash.frompairs": "^4.0.7",
    "@types/lodash.groupby": "^4.6.7",
    "@types/lodash.mapvalues": "^4.6.7",
    "@types/node": "^18.7.13",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "eslint": "^8.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": ">=6",
    "jest": "^29.0.2",
    "lint-staged": ">=10",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.8",
    "tsup": "^6.2.3",
    "typescript": "^4.8.2"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "eslint --fix --quiet",
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "aries",
    "swagger",
    "converter",
    "typescript",
    "markdown",
    "mock",
    "schema",
    "cli"
  ]
}
