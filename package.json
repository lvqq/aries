{
  "name": "@tooltik/aries",
  "type": "module",
  "version": "0.11.1",
  "description": "A swagger converter cli, support TypeScript declaration, request function, markdown docs, mock js and so on",
  "main": "./dist/src/index.cjs",
  "module": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "bin": {
    "aries": "./dist/bin/aries.js"
  },
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js",
      "require": "./dist/src/index.cjs"
    }
  },
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "dev": "rimraf dist && tsup --sourcemap --watch",
    "build": "rimraf dist && tsup --sourcemap",
    "build:production": "rimraf dist && tsup --treeshake",
    "prepare": "husky install",
    "prepublish": "pnpm build:production",
    "test-gen:to-ts": "aries to-ts -u ./test/swagger.json -o ./test/output.swagger.types.ts",
    "test-gen:to-request": "aries to-request -u ./test/swagger.json -o ./test/output.swagger.request.ts",
    "test-gen:to-md": "aries to-md -u ./test/swagger.json -o ./test/output.swagger.docs.md",
    "test-gen:to-mock": "aries to-mock -u ./test/swagger.json -o ./test/output.swagger.mock.json",
    "test-gen:yml:to-ts": "aries to-ts -u ./test/swagger.yml -o ./test/output.yml.swagger.types.ts",
    "test-gen:yml:to-request": "aries to-request -u ./test/swagger.yml -o ./test/output.yml.swagger.request.ts",
    "test-gen:yml:to-md": "aries to-md -u ./test/swagger.yml -o ./test/output.yml.swagger.docs.md",
    "test-gen:yml:to-mock": "aries to-mock -u ./test/swagger.yml -o ./test/output.yml.swagger.mock.json",
    "test-gen:node": "node ./test/node-gen.cjs",
    "test-server:mock-server": "aries mock-server -u ./test/swagger.json",
    "test-server:yml:mock-server": "aries mock-server -u ./test/swagger.yml",
    "test-gen": "run-s test-gen:**",
    "lint": "eslint --fix --quiet --ext .ts src bin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lvqq/aries.git"
  },
  "author": "lvqq",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lvqq/aries/issues"
  },
  "homepage": "https://github.com/lvqq/aries#readme",
  "files": [
    "dist"
  ],
  "dependencies": {
    "axios": "^0.24.0",
    "chalk": "4.1.2",
    "chance": "^1.1.8",
    "commander": "^8.3.0",
    "cors": "^2.8.5",
    "esbuild": "^0.15.6",
    "express": "^4.17.2",
    "js-yaml": "^4.1.0",
    "lodash.frompairs": "^4.0.1",
    "lodash.groupby": "^4.6.0",
    "lodash.mapvalues": "^4.6.0",
    "ora": "5.4.1"
  },
  "devDependencies": {
    "@types/chance": "^1.1.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.184",
    "@types/lodash.frompairs": "^4.0.7",
    "@types/lodash.groupby": "^4.6.7",
    "@types/lodash.mapvalues": "^4.6.7",
    "@types/node": "^18.7.13",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "eslint": "^8.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "tsup": "^6.2.3",
    "typescript": "^4.8.2"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "eslint --fix --quiet",
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "aries",
    "swagger",
    "converter",
    "typescript",
    "markdown",
    "mock",
    "schema",
    "cli"
  ]
}
