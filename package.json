{
  "name": "@tooltik/aries",
  "version": "0.9.0",
  "description": "A swagger converter cli, support TypeScript declaration, markdown docs, mock js and so on",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "bin": {
    "aries": "./bin/aries.js"
  },
  "scripts": {
    "test-gen:to-ts": "aries to-ts -u ./test/swagger.json -o ./test/output.swagger.types.ts",
    "test-gen:to-md": "aries to-md -u ./test/swagger.json -o ./test/output.swagger.docs.md",
    "test-gen:to-mock": "aries to-mock -u ./test/swagger.json -o ./test/output.swagger.mock.json",
    "test-gen:yml:to-ts": "aries to-ts -u ./test/swagger.yml -o ./test/output.yml.swagger.types.ts",
    "test-gen:yml:to-md": "aries to-md -u ./test/swagger.yml -o ./test/output.yml.swagger.docs.md",
    "test-gen:yml:to-mock": "aries to-mock -u ./test/swagger.yml -o ./test/output.yml.swagger.mock.json",
    "test-gen:node": "node ./test/node-gen",
    "test-server:mock-server": "aries mock-server -u ./test/swagger.json",
    "test-server:yml:mock-server": "aries mock-server -u ./test/swagger.yml",
    "test-gen": "run-s test-gen:**",
    "lint": "eslint --fix --quiet --ext js src bin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lvqq/aries.git"
  },
  "author": "lvqq",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lvqq/aries/issues"
  },
  "homepage": "https://github.com/lvqq/aries#readme",
  "files": [
    "src",
    "bin"
  ],
  "dependencies": {
    "axios": "^0.24.0",
    "chalk": "4.1.2",
    "chance": "^1.1.8",
    "commander": "^8.3.0",
    "cors": "^2.8.5",
    "express": "^4.17.2",
    "js-yaml": "^4.1.0",
    "lodash.frompairs": "^4.0.1",
    "lodash.groupby": "^4.6.0",
    "lodash.mapvalues": "^4.6.0",
    "lodash.omit": "^4.5.0",
    "ora": "5.4.1"
  },
  "devDependencies": {
    "eslint": "^8.5.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1"
  },
  "lint-staged": {
    "src/**/*.{js,ts}": [
      "eslint --fix --quiet",
      "prettier --write",
      "git add"
    ]
  },
  "keywords": [
    "aries",
    "swagger",
    "converter",
    "typescript",
    "markdown",
    "mock",
    "schema",
    "cli"
  ]
}
