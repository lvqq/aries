{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "airbnb-base",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 13
  },
  "rules": {
    "prettier/prettier": "error",
    "import/no-dynamic-require": 0,
    "global-require": 0,
    "no-unused-vars": 1,
    "class-methods-use-this": 0,
    "no-console": 0
  }
}
